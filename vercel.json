{
    "installCommand": "npm install",
    "buildCommand": "npm run build",
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "**/*.ts",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {"src": "/(.*)", "dest": "/"}
    ]
}